    TPair<FString, FString> ContentType;
    {{#pathParams}}
    TPair<FString, {{dataType}}> Param;
    Param.Key = FString("{" + "{{paramName}}" + "}");
    Param.Value = {{paramName}}
    ConstructPathParams(Param, BasePath);
    {{/pathParams}}
    {{#bodyParams}}
    
    {{#isListContainer}}
        {{#items}}
            {{#isModel}}
    ContentType.Key = "content-type";
    ContentType.Value = "application/json";
            {{/isModel}}
        {{/items}}
    {{/isListContainer}}
    {{#isModel}}
    ContentType.Key = "content-type";
    ContentType.Value = "application/json";
    {{/isModel}}
    {{/bodyParams}}

    CreateRequest(EVerbType::{{httpMethod}}, BasePath, Headers, FHttpRequestCompleteDelegate());
    HttpRequest->OnProcessRequestComplete().BindRaw(this, &{{classname}}::On{{operationId}}, SuccessDelegate, ErrorDelegate);
    return HttpRequest->ProcessRequest();
}
