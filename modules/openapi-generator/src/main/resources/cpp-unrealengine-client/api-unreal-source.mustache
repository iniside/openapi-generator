    {{#isListContainer}}
   
	{
        TPair<FString,{{dataType}}> statusParam;
	    statusParam.Key = "{{baseName}}";
	    for({{baseType}} p : {{paramName}})
        {
            statusParam.Value.Add(p);
        }
        Params.Add(statusParam);
    }
    {{/isListContainer}}
    {{^isListContainer}}
    {
        TPair<FString, TArray<{{dataType}}>> statusParam;
